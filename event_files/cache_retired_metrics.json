[
    {
        "MetricName": "CACHE_RETIRED.L1D.LOAD.MPKI",
        "BriefDescription": "The rate of retired L1 data load cache misses per kilo instructions retired",
        "MetricExpr": "(CACHE_RETIRED.L1D.LOAD.MISS + CACHE_RETIRED.L1D.STORE.MISS) / RETIRED.INST * 1000",
        "MetricGroup": "Cache",
        "ScaleUnit": "1PKI"
    },
    {
        "MetricName": "CACHE_RETIRED.L1D.LOAD.MISS_RATE",
        "BriefDescription": "The ratio of retired L1D cache load misses to the total number of retired L1D load accesses",
        "MetricExpr": "CACHE_RETIRED.L1D.LOAD.MISS / CACHE_RETIRED.L1D.LOAD.ACCESS",
        "MetricGroup": "Cache",
        "ScaleUnit": "1PKI"
    }
]