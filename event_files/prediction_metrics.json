    {
        "MetricName": "control_flow_pki",
        "BriefDescription": "The rate of control flow instructions retired per kilo instructions",
        "MetricExpr": "PRD.CONTROL_FLOW.RETIRED / GEN.INST_RETIRED * 1000",
        "MetricGroup": "Speculation",
        "ScaleUnit": "1PKI"
    },
    {
